<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox spacing="10" xmlns:fx="http://javafx.com/fxml">
    <CheckBox fx:id="dailyChecklist" text="Daily Safety Checklist" />
    <CheckBox fx:id="reportIssues" text="Report Safety Issues" />
    <CheckBox fx:id="viewInstructions" text="View Safety Instructions" />
    <TableView fx:id="complianceProgressTable">
        <columns>
            <TableColumn text="Standard" />
            <TableColumn text="Compliance" />
        </columns>
    </TableView>
    <Button fx:id="submitChecklistButton" text="Submit Checklist" />
</VBox>
