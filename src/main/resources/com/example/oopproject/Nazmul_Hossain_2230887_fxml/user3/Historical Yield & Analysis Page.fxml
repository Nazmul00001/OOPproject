<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox xmlns:fx="http://javafx.com/fxml" fx:controller="com.example.oopproject.Nazmul_Hossain_2230887_controler.user3.HistoricalYieldController" spacing="10" alignment="CENTER">
    <children>
        <TableView fx:id="historicalDataTable" prefWidth="400" prefHeight="200">
            <columns>
                <TableColumn text="Year"/>
                <TableColumn text="Yield"/>
                <TableColumn text="Factors"/>
            </columns>
        </TableView>
        <TextField fx:id="landDetails" promptText="Enter Land Details"/>
        <TextField fx:id="equipmentDetails" promptText="Enter Equipment Details"/>
        <Button fx:id="submitPlanButton" text="Submit Plan" onAction="#submitPlan"/>
        <ComboBox fx:id="analysisFactors" promptText="Select Factor for Analysis"/>
        <Button fx:id="generateGraphButton" text="Generate Graph" onAction="#generateGraph"/>
    </children>
</VBox>
