<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox spacing="10" xmlns:fx="http://javafx.com/fxml">
    <TableView fx:id="complianceData">
        <columns>
            <TableColumn text="Requirement" />
            <TableColumn text="Status" />
        </columns>
    </TableView>
    <HBox spacing="10">
        <Button fx:id="runChecksButton" text="Run Compliance Checks" />
        <Button fx:id="generateReportButton" text="Generate Report" />
        <Button fx:id="downloadReportButton" text="Download Report" />
    </HBox>
</VBox>
