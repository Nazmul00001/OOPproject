<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox xmlns:fx="http://javafx.com/fxml" fx:controller="com.example.oopproject.Nazmul_Hossain_2230887.user4.TrainingModulePageController" spacing="10" alignment="CENTER">
    <children>
        <ComboBox fx:id="trainingSessionSelection" promptText="Select Training Session"/>
        <TextField fx:id="quizInputField" promptText="Enter Quiz Answer"/>
        <HBox spacing="10" alignment="CENTER">
            <Button fx:id="submitQuizButton" text="Submit Quiz" onAction="#submitQuiz"/>
            <Button fx:id="markTrainingCompleteButton" text="Mark Training Complete" onAction="#markTrainingComplete"/>
        </HBox>
    </children>
</VBox>
