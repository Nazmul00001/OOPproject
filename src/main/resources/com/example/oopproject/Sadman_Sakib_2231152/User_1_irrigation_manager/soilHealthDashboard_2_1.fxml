<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="658.0" prefWidth="647.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/23" fx:controller="com.example.oopproject.Sadman_Sakib_2231152.User_1_irrigation_manager.SoilHealthDashboardController_2_1">
    <!-- Label for Soil Moisture Data -->
    <Label layoutX="50" layoutY="20" text="Soil Moisture Data" />
    <TableView fx:id="soilMoistureTable" layoutX="50" layoutY="50" prefHeight="150" prefWidth="200">
        <columns>
            <TableColumn fx:id="timeColumnSoilMoisture" prefWidth="76.0" text="Time" />
            <TableColumn fx:id="moistureLevelColumn" prefWidth="122.0" text="Moisture Level" />
        </columns>
    </TableView>

    <!-- Label for pH Data -->
    <Label layoutX="300" layoutY="20" text="pH Level Data" />
    <TableView fx:id="phTable" layoutX="300" layoutY="50" prefHeight="150" prefWidth="200">
        <columns>
            <TableColumn fx:id="timeColumnPH" text="Time" />
            <TableColumn fx:id="pHLevelColumn" prefWidth="119.0" text="pH Level" />
        </columns>
    </TableView>

    <!-- Label for Nutrient Levels Data -->
    <Label layoutX="50" layoutY="220" text="Nutrient Levels Data" />
    <TableView fx:id="nutrientLevelsTable" layoutX="50" layoutY="250" prefHeight="150" prefWidth="248.0">
        <columns>
            <TableColumn fx:id="timeColumnNutrientLevels" prefWidth="107.0" text="Time" />
            <TableColumn fx:id="nutrientLevelsColumn" prefWidth="140.0" text="Nutrient Levels" />
        </columns>
    </TableView>

    <!-- Table for Soil Test Results -->
    <Label layoutX="50" layoutY="420" text="Soil Test Results" />
    <TableView fx:id="soilTestResultsTable" layoutX="50" layoutY="450" prefHeight="150" prefWidth="500">
        <columns>
            <TableColumn fx:id="testDateColumn" prefWidth="98.0" text="Test Date" />
            <TableColumn fx:id="moistureLevelColumnResults" prefWidth="134.0" text="Moisture Level" />
            <TableColumn fx:id="pHLevelColumnResults" prefWidth="112.0" text="pH Level" />
            <TableColumn fx:id="nutrientLevelsColumnResults" prefWidth="155.0" text="Nutrient Levels" />
        </columns>
    </TableView>

    <!-- Button for Viewing Detailed Reports -->
    <Button layoutX="50" layoutY="620" onAction="#viewReportsButton" text="View Detailed Reports" />
</AnchorPane>
